<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="de.nordakademie.roommgmt.model.Room" table="ROOM">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="building" not-null="true"/>
		<property name="roomNumber" column="ROOM_NUMBER" not-null="true"/>
		<property name="seatCount" not-null="true"/>
		<property name="beamerPresent" column="BEAMER" not-null="true" />
	</class>
</hibernate-mapping>